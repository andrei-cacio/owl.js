{
  "name": "owl.js",
  "version": "0.1.0",
  "description": "Backbone-like frontend library",
  "main": "index.js",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "test": "./node_modules/.bin/karma start --single-run --browsers PhantomJS",
    "docs": "./node_modules/.bin/mr-doc -s src/ -o docs/ -n 'owl.js' -t 'cayman'",
    "tsdocs": "./node_modules/.bin/typedoc --out tsdocs/ typescript/owl.d.ts --includeDeclarations --theme minimal --module system --mode file"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/omegascorp/owl.git"
  },
  "author": "",
