    "url": "https://github.com/omegascorp/owl/issues"
  },
  "homepage": "https://github.com/omegascorp/owl#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "karma": "^0.13.22",
    "karma-coverage": "^0.5.5",
    "karma-coveralls": "^1.1.2",
    "karma-mocha": "^0.2.2",
    "karma-mocha-reporter": "^2.0.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sinon-chai": "^1.2.0",
    "lolex": "^1.4.0",
    "mocha": "^2.4.5",
    "mr-doc": "^3.2.1",
    "mr-doc-theme-cayman": "^1.4.0",
